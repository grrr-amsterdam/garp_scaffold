<?xml version="1.0" encoding="UTF-8" ?>
<class xmlns="http://xml.phpdox.net/src" full="DirectoryIterator" namespace="" name="DirectoryIterator">
  <implements name="SeekableIterator" full="SeekableIterator"/>
  <constructor name="__construct" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Constructs a new directory iterator from a path"/>
      <return type="void"/>
    </docblock>
    <parameter name="path" optional="false" byreference="false" type="string"/>
  </constructor>
  <method name="current" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Return the current DirectoryIterator item."/>
      <return type="DirectoryIterator"/>
    </docblock>
  </method>
  <method name="getATime" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get last access time of the current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getBasename" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get base name of current DirectoryIterator item."/>
      <return type="string"/>
    </docblock>
    <parameter name="suffix" optional="true" byreference="false" type="string"/>
  </method>
  <method name="getCTime" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get inode change time of the current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getExtension" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Gets the file extension"/>
      <return type="string"/>
    </docblock>
  </method>
  <method name="getFilename" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Return file name of current DirectoryIterator item."/>
      <return type="string"/>
    </docblock>
  </method>
  <method name="getGroup" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get group for the current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getInode" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get inode for the current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getMTime" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get last modification time of current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getOwner" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get owner of current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getPath" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get path of current Iterator item without filename"/>
      <return type="string"/>
    </docblock>
  </method>
  <method name="getPathname" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Return path and file name of current DirectoryIterator item"/>
      <return type="string"/>
    </docblock>
  </method>
  <method name="getPerms" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get the permissions of current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getSize" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get size of current DirectoryIterator item"/>
      <return type="int"/>
    </docblock>
  </method>
  <method name="getType" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine the type of the current DirectoryIterator item"/>
      <return type="string"/>
    </docblock>
  </method>
  <method name="isDir" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine if current DirectoryIterator item is a directory"/>
      <return type="bool"/>
    </docblock>
  </method>
  <method name="isDot" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine if current DirectoryIterator item is '.' or '..'"/>
      <return type="bool"/>
    </docblock>
  </method>
  <method name="isExecutable" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine if current DirectoryIterator item is executable"/>
      <return type="bool"/>
    </docblock>
  </method>
  <method name="isFile" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine if current DirectoryIterator item is a regular file"/>
      <return type="bool"/>
    </docblock>
  </method>
  <method name="isLink" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine if current DirectoryIterator item is a symbolic link"/>
      <return type="bool"/>
    </docblock>
  </method>
  <method name="isReadable" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine if current DirectoryIterator item can be read"/>
      <return type="bool"/>
    </docblock>
  </method>
  <method name="isWritable" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Determine if current DirectoryIterator item can be written to"/>
      <return type="bool"/>
    </docblock>
  </method>
  <method name="key" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Return the key for the current DirectoryIterator item"/>
      <return type="string"/>
    </docblock>
  </method>
  <method name="next" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Move forward to next DirectoryIterator item"/>
      <return type="void"/>
    </docblock>
  </method>
  <method name="rewind" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Rewind the DirectoryIterator back to the start"/>
      <return type="void"/>
    </docblock>
  </method>
  <method name="seek" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Seek to a DirectoryIterator item"/>
      <return type="void"/>
    </docblock>
    <parameter name="position" optional="false" byreference="false" type="int"/>
  </method>
  <method name="__toString" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Get file name as a string"/>
      <return type="string"/>
    </docblock>
  </method>
  <method name="valid" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Check whether current DirectoryIterator position is a valid file"/>
      <return type="bool"/>
    </docblock>
  </method>
</class>
