<?xml version="1.0" encoding="UTF-8" ?>
<class xmlns="http://xml.phpdox.net/src" full="Directory" namespace="" name="Directory">
  <member name="path" static="false" visibility="public">
    <default>null</default>
    <docblock>
      <description compact="&#10;       The directory that was opened.&#10;      "/>
      <var type="string"/>
    </docblock>
  </member>
  <member name="handle" static="false" visibility="public">
    <default>null</default>
    <docblock>
      <description compact="&#10;       Can be used with other directory functions such as&#10;       readdir, rewinddir and&#10;       closedir.&#10;      "/>
      <var type="resource"/>
    </docblock>
  </member>
  <method name="close" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Close directory handle"/>
      <return type="void"/>
    </docblock>
    <parameter name="dir_handle" optional="true" byreference="false" type="object" class="resource"/>
  </method>
  <method name="read" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Read entry from directory handle"/>
      <return type="string"/>
    </docblock>
    <parameter name="dir_handle" optional="true" byreference="false" type="object" class="resource"/>
  </method>
  <method name="rewind" abstract="false" static="false" visibility="public" final="false">
    <docblock>
      <description compact="Rewind directory handle"/>
      <return type="void"/>
    </docblock>
    <parameter name="dir_handle" optional="true" byreference="false" type="object" class="resource"/>
  </method>
</class>
