<?xml version="1.0" encoding="UTF-8" ?>
<class xmlns="http://xml.phpdox.net/src" full="SQLite3" namespace="" name="SQLite3">
  <method name="busyTimeout" abstract="false" static="false" visibility="public" final="false">
