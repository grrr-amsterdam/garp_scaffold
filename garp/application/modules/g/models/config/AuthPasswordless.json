{
	"module": "garp",

	"inputs": {
		"token": {
		},
		"token_expiration_date": {
			"type": "datetime"
		},
		"claimed": {
			"type": "checkbox",
			"default": 0
		},
		"ip_address": {
			"maxLength": 15,
			"required": false
		},
		"last_login": {
			"type": "datetime",
			"required": false
		}
	},

	"listFields": ["id", "ip_address"],

	"relations": {
		"User": {
			"type": "belongsTo"
		}
	}
}
